{
  "name": "siwalik-stratigraphy",
  "version": "0.0.1",
  "description": "Scripts for the siwalik site",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compress:subregional": "npx svgo 'svg/Subregional Key Map.svg' -o 'svg/Subregional_Key_Map_Compressed.svg' --pretty --disable=convertStyleToAttrs --disable=inlineStyles",
    "compress:regional": "npx svgo 'svg/Regional Key Map.svg' -o 'svg/Regional_Key_Map_Compressed.svg' --pretty --disable=convertStyleToAttrs --disable=inlineStyles",
    "compress:potwar": "npx svgo 'svg/Potwar Map.svg' -o 'svg/Potwar_Map_Compressed.svg' --pretty --enable=prefixIds --disable=convertStyleToAttrs --disable=inlineStyles",
    "compress:soan": "npx svgo 'svg/Soan Sections.svg' -o 'svg/Soan_Sections_Compressed.svg' --pretty --enable=prefixIds --disable=convertStyleToAttrs --disable=inlineStyles",
    "compress:eastern_potwar_sections": "npx svgo 'svg/Eastern Potwar Sections.svg' -o 'svg/Eastern_Potwar_Sections_Compressed.svg' --pretty --enable=prefixIds --disable=convertStyleToAttrs --disable=inlineStyles",
    "compress:basawa_sections": "npx svgo 'svg/Basawa Sections.svg' -o 'svg/Basawa_Sections_Compressed.svg' --pretty --enable=prefixIds --disable=convertStyleToAttrs --disable=inlineStyles",
    "compress:chambal_sections": "npx svgo 'svg/Chambal Sections.svg' -o 'svg/Chambal_Sections_Compressed.svg' --pretty --enable=prefixIds --disable=convertStyleToAttrs --disable=inlineStyles",
    "compress:north_rohtas_sections": "npx svgo 'svg/North Rohtas Sections.svg' -o 'svg/_Sections_Compressed.svg' --pretty --enable=prefixIds --disable=convertStyleToAttrs --disable=inlineStyles",
    "compress:mirpur_sections": "npx svgo 'svg/New Mirpur Sections.svg' -o 'svg/New Mirpur_Sections_Compressed.svg' --pretty --enable=prefixIds --disable=convertStyleToAttrs --disable=inlineStyles",
    "compress:kashmir": "npx svgo 'svg/Kashmir Map.svg' -o 'svg/Kashmir_Compressed.svg' --pretty --enable=prefixIds --disable=convertStyleToAttrs --disable=inlineStyles",
    "compress:index": "npx svgo 'svg/Index Map.svg' -o 'svg/Index_Map_Compressed.svg'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aprilbender/siwalik.git"
  },
  "author": "April Johnson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aprilbender/siwalik/issues"
  },
  "homepage": "https://github.com/aprilbender/siwalik#readme",
  "dependencies": {
    "svgo": "^1.1.1"
  }
}
